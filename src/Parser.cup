import java_cup.runtime.*;

/* TERMINAL */
terminal
	ECHO, SEMICOLON
;

terminal String
	TOKEN
;

/* NON TERMINAL */
non terminal String
	statement_list, statement, token_list
;


/* PRODUCTION */
statement_list ::=
	statement
	| statement_list statement
;

statement ::=
	ECHO token_list:tl SEMICOLON
	{: System.out.println(tl); :}
;

token_list ::=
	TOKEN:t 
	{: RESULT = t; :}
	| token_list:tl TOKEN:t
	{: RESULT = tl + " " + t; :}
;





