import java_cup.runtime.*;

terminal           PLUS, TIMES;
terminal Integer   NUMBER;

non terminal Integer    expr;

precedence left PLUS, TIMES;

expr      ::= expr:e1 PLUS expr:e2
	      {: RESULT = e1 + e2; :}
	      | expr:e1 TIMES expr:e2
	      {: RESULT = e1 * e2; :}
	      | NUMBER:n
	      {: RESULT = n; :} 
	      ;
